{
    "name": "joaomfrebelo/saft-pt_4_php",
    "description": "PHP core lib to work with SAFT-PT. A library/framework to generate, parse, import and validate programmatically or cli the Portuguese Tax Audit file SAFT-PT",
    "homepage": "https://github.com/joaomfrebelo/saft-pt_4_php",
    "version": "2.0.2",
    "keywords": [
        "SAFT-PT",
        "AT",
        "Autoridade tributaria",
        "e-fatura",
        "Portuguese Tax Authority",
        "Portuguese tax file audit"
    ],
    "authors": [
        {
            "name": "Jo√£o M F Rebelo",
            "email": "joaomfrebelo@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/",
            "tests/"
        ],
        "files": [
            "bootstrap.php"
        ]
    },
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "apache/log4php": "^2.3.0",
        "joaomfrebelo/date": "^2.2.2",
        "joaomfrebelo/decimal": "^4.0.0",
        "joaomfrebelo/enum": "^2.0.4",
        "symfony/console": "^v6.2.3",
        "spatie/async": "^1.5.5",
        "symfony/process": "^v6.2.0",
        "joaomfrebelo/composer-revision-plugin": "^2.0.1",
	  	"ext-simplexml": "*"
	},
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "type": "library",
    "bin": [
        "saft4php"
    ],
    "config": {
        "allow-plugins": {
            "joaomfrebelo/composer-revision-plugin": true
        }
    }
}
